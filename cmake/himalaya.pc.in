prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=@CMAKE_INSTALL_FULL_LIBDIR@
includedir=@CMAKE_INSTALL_FULL_INCLUDEDIR@

Name: Himalaya
Description: C++ library with 3-loop Higgs mass corrections in the MSSM
Version: @PROJECT_VERSION@
Requires: eigen3 >= 3.0.0
Libs: -L${libdir} -lHimalaya -lDSZ
Cflags: -I${includedir}
